{
	"name": "getafix",
	"displayName": "getafix",
	"description": "webhooks on vscode",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.41.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:getfix.helloWorld",
		"onCommand:getfix.start"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "getfix.helloWorld",
				"title": "Hello World"
			},
			{
				"command": "getfix.start",
				"title": "HTTP Inspector"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/request-promise-native": "^1.0.17",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"tslint": "^5.20.0",
		"typescript": "^3.6.4",
		"vscode-test": "^1.2.2"
	},
	"dependencies": {
		"request": "^2.88.0",
		"request-promise-native": "^1.0.8"
	}
}
